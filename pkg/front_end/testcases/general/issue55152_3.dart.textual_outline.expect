class Class {
  const Class.named(
      {dynamic x = (({dynamic y = const [Alias2.impl()]}) =>
          const [Alias.impl()])});
}

typedef Alias<X> = Const<X>;

typedef Alias2<X> = Const<X>;

abstract class Const<X> {
  const factory Const.impl() = _ConstImpl;
}

class _ConstImpl<T> implements Const<T> {
  const _ConstImpl();
}
